import json
import os
import time
from datetime import datetime, timedelta
from zoneinfo import ZoneInfo
from bs4 import BeautifulSoup
from playwright.sync_api import sync_playwright

JUMP_URL = "https://www.moely.link/random/jump/?wallpaper=true"
JSON_PATH = "wallpaper.json"

def log(msg: str):
    now = datetime.now(ZoneInfo("Asia/Shanghai")).strftime("%Y-%m-%d %H:%M:%S")
    print(f"[{now}] {msg}")

def fetch_wallpaper_data():
    """ä½¿ç”¨ Playwright æ‰“å¼€é¡µé¢å¹¶è·å– <script id="wallpaper"> ä¸­çš„ JSON æ•°æ®"""
    with sync_playwright() as p:
        browser = p.chromium.launch(headless=True)
        page = browser.new_page()
        log(f"â–¶ï¸ æ‰“å¼€ {JUMP_URL}")
        page.goto(JUMP_URL, timeout=30000, wait_until="domcontentloaded")
        # ç­‰å¾…é¡µé¢å‘ç”Ÿè·³è½¬
        page.wait_for_event("framenavigated", timeout=10000)
        print("âœ… é¡µé¢å·²è·³è½¬åˆ°ï¼š", page.url)
        # ç­‰å¾…ç›®æ ‡å…ƒç´ åŠ è½½
        time.sleep(3)
        html = page.content()
        browser.close()

    soup = BeautifulSoup(html, "html5lib")
    script = soup.find("script", {"id": "wallpaper"})
    if script and script.string:
        return json.loads(script.string)
    else:
        return None

def load_existing():
    if os.path.exists(JSON_PATH):
        with open(JSON_PATH, "r", encoding="utf-8") as f:
            return json.load(f)
    return []

def save_all(data_list):
    with open(JSON_PATH, "w", encoding="utf-8") as f:
        json.dump(data_list, f, ensure_ascii=False, indent=2)
    log(f"âœ… ä¿å­˜ {JSON_PATH}ï¼Œå…± {len(data_list)} æ¡è®°å½•")

def main():
    tz = ZoneInfo("Asia/Shanghai")
    today = datetime.now(tz).date()
    target_date = today + timedelta(days=7)
    date_str = target_date.strftime("%Y%m%d")

    log("ğŸš€ å¼€å§‹è·å–éšæœºå£çº¸ä¿¡æ¯")
    info = fetch_wallpaper_data()
    if not info:
        log("âŒ æœªèƒ½è·å–åˆ° wallpaper æ•°æ®ï¼Œç¨‹åºç»ˆæ­¢")
        return

    info["date"] = date_str
    log(f"âœ… è·å–åˆ° wallpaper ID={info.get('id')}ï¼Œç›®æ ‡æ—¥æœŸ={date_str}")

    items = load_existing()

    # åŸºäº date åˆ¤æ–­æ˜¯å¦å·²å­˜åœ¨
    if not any(item.get("date") == date_str for item in items):
        items.append(info)
        log("ğŸ¨ å£çº¸æ·»åŠ æˆåŠŸ")
    else:
        log("â„¹ï¸ å£çº¸å·²å­˜åœ¨ï¼Œä¸é‡å¤æ·»åŠ ")

    # è¿‡æ»¤ä¿ç•™ [today-7 .. today+7]
    min_date = today - timedelta(days=7)
    filtered = [
        it for it in items
        if min_date <= datetime.strptime(it["date"], "%Y%m%d").date() <= target_date
    ]
    filtered.sort(key=lambda x: x["date"])
    save_all(filtered)

if __name__ == "__main__":
    main()
addEventListener('fetch', event => {
  event.respondWith(handleRequest(event.request))
})

async function handleRequest(request) {
  const apiUrl = 'https://cn.bing.com/HPImageArchive.aspx?format=js&idx=0&n=1&mkt=zh-CN'
  const url = new URL(request.url)
  const resParam = url.searchParams.get('res') || 'uhd'

  try {

    const response = await fetch(apiUrl)
    const data = await response.json()
    
    // Check
    const image = data.images[0]
    if (!image) throw new Error('No image found')
    if (!image.urlbase) throw new Error('Missing image base URL')

    let suffix
    switch (resParam.toLowerCase()) {
      case 'uhd':
        suffix = '_UHD.jpg'       // è¶…é«˜æ¸… 3840x2160
        break
      case 'm':
        suffix = '_1080x1920.jpg' // æ‰‹æœºç«–ç‰ˆ 1080x1920
        break
      default:
        // è‡ªå®šä¹‰åˆ†è¾¨ç‡æ ¼å¼ (1920x1080)
        if (/^\d+x\d+$/.test(resParam)) {
          suffix = `_${resParam}.jpg`
        } else {
          throw new Error(`ILOVESCRATCH API ERROR: UNKNOWN RESOLUTION ${resParam}`)
        }
    }

    const imageUrl = `https://cn.bing.com${image.urlbase}${suffix}`
    

    if (!isValidUrl(imageUrl)) throw new Error('ILOVESCRATCH API ERROR(INTERNAL): INVALID URL GENERATED.    IF YOU BELIEVE THIS IS A MISTAKE, CONTACT ILOVESCRATCH@FOXMAIL.com')


    return Response.redirect(imageUrl, 302)

  } catch (error) {
    return new Response(error.message, {
      status: error.message.startsWith('ILOVESCRATCH API ERROR:') ? 400 : 500,
      headers: { 'Content-Type': 'text/plain' }
    })
  }
}

function isValidUrl(string) {
  try {
    new URL(string)
    return true
  } catch (_) {
    return false
  }
}
// EdgeOneè¾¹ç¼˜å‡½æ•° - Bingå£çº¸APIä¸å›¾ç‰‡ä»£ç†
addEventListener('fetch', event => {
  event.respondWith(handleRequest(event.request));
});

async function handleRequest(request) {
  const url = new URL(request.url);
  const pathname = url.pathname;
  
  // æ ¹æ®è·¯å¾„åˆ¤æ–­è¯·æ±‚ç±»å‹
  if (pathname === '/' || pathname === '') {
    // æ ¹ç›®å½•è®¿é—® - è¿”å›å›¾ç‰‡
    return handleImageRequest(request);
  } else if (pathname.startsWith('/api') || pathname.includes('api')) {
    // APIè®¿é—® - è¿”å›JSONæ•°æ®
    return handleApiRequest(request);
  } else {
    // å…¶ä»–è·¯å¾„ - è¿”å›404
    return new Response('Not Found', { status: 404 });
  }
}

// å¤„ç†å›¾ç‰‡è¯·æ±‚ - ç›´æ¥è¿”å›å›¾ç‰‡
async function handleImageRequest(request) {
  // å¤„ç†é¢„æ£€è¯·æ±‚
  if (request.method === 'OPTIONS') {
    return new Response(null, {
      status: 200,
      headers: getCorsHeaders(),
    });
  }

  try {
    const url = new URL(request.url);
    const params = url.searchParams;
    
    // è·å–å‚æ•°
    const idx = params.get('idx') || '0';
    const mkt = params.get('mkt') || 'zh-CN';
    const resolution = params.get('resolution') || '1920x1080';
    
    console.log(`å›¾ç‰‡è¯·æ±‚ - idx: ${idx}, mkt: ${mkt}, resolution: ${resolution}`);
    
    // é¦–å…ˆè·å–å›¾ç‰‡ä¿¡æ¯
    const imageData = await getBingImageData(idx, mkt, 1);
    
    if (!imageData || !imageData.images || imageData.images.length === 0) {
      return createErrorResponse('æ— æ³•è·å–Bingå£çº¸ä¿¡æ¯', 404);
    }
    
    const image = imageData.images[0];
    let imageUrl;
    
    // æ ¹æ®åˆ†è¾¨ç‡å‚æ•°é€‰æ‹©å¯¹åº”çš„å›¾ç‰‡URL
    if (resolution === 'uhd' || resolution === '4k') {
      imageUrl = `https://www.bing.com${image.urlbase}_UHD.jpg`;
    } else if (resolution === 'mobile') {
      imageUrl = `https://www.bing.com${image.urlbase}_768x1366.jpg`;
    } else if (resolution === 'thumbnail') {
      imageUrl = `https://www.bing.com${image.urlbase}_400x240.jpg`;
    } else {
      // é»˜è®¤æˆ–è‡ªå®šä¹‰åˆ†è¾¨ç‡
      imageUrl = `https://www.bing.com${image.urlbase}_${resolution}.jpg`;
    }
    
    console.log(`ä»£ç†å›¾ç‰‡URL: ${imageUrl}`);
    
    // ä»£ç†å›¾ç‰‡è¯·æ±‚ - å¸¦é‡è¯•æœºåˆ¶
    const imageResponse = await fetchImageWithRetry(imageUrl);
    
    if (!imageResponse.ok) {
      console.error(`å›¾ç‰‡è¯·æ±‚å¤±è´¥: ${imageResponse.status}`);
      return createErrorResponse(`å›¾ç‰‡åŠ è½½å¤±è´¥: ${imageResponse.status}`, 502);
    }
    
    // è·å–å›¾ç‰‡æ•°æ®
    const imageBuffer = await imageResponse.arrayBuffer();
    const contentType = imageResponse.headers.get('content-type') || 'image/jpeg';
    
    // æ„å»ºå“åº”å¤´
    const headers = {
      ...getCorsHeaders(),
      'Content-Type': contentType,
      'Content-Length': imageBuffer.byteLength.toString(),
      'Cache-Control': 'public, max-age=86400, s-maxage=172800', // ç¼“å­˜24å°æ—¶ï¼ŒEdgeOneç¼“å­˜48å°æ—¶
      'Last-Modified': new Date().toUTCString(),
      'ETag': `"bing-${idx}-${resolution}-${Date.now()}"`,
      // å›¾ç‰‡ä¿¡æ¯å¤´
      'X-Image-Title': encodeURIComponent(image.title || ''),
      'X-Image-Copyright': encodeURIComponent(image.copyright || ''),
      'X-Image-Date': image.startdate || '',
      'X-Resolution': resolution,
      'X-Powered-By': 'EdgeOne-Image-Proxy',
    };
    
    console.log(`æˆåŠŸè¿”å›å›¾ç‰‡ï¼Œå¤§å°: ${imageBuffer.byteLength} bytes`);
    
    return new Response(imageBuffer, {
      status: 200,
      headers: headers,
    });
    
  } catch (error) {
    console.error('å›¾ç‰‡è¯·æ±‚å¤„ç†é”™è¯¯:', error);
    return createErrorResponse(`å›¾ç‰‡æœåŠ¡é”™è¯¯: ${error.message}`, 500);
  }
}

// å¤„ç†APIè¯·æ±‚ - è¿”å›JSONæ•°æ®
async function handleApiRequest(request) {
  // å¤„ç†é¢„æ£€è¯·æ±‚
  if (request.method === 'OPTIONS') {
    return new Response(null, {
      status: 200,
      headers: getCorsHeaders(),
    });
  }

  try {
    const url = new URL(request.url);
    const params = url.searchParams;
    
    // è·å–æŸ¥è¯¢å‚æ•°
    const format = params.get('format') || 'js';
    const idx = params.get('idx') || '0';
    const n = Math.min(parseInt(params.get('n')) || 1, 8);
    const mkt = params.get('mkt') || 'zh-CN';
    const resolution = params.get('resolution') || '1920x1080';
    
    console.log(`APIè¯·æ±‚ - format: ${format}, idx: ${idx}, n: ${n}, mkt: ${mkt}`);
    
    // å‚æ•°éªŒè¯
    if (!/^(js|xml)$/.test(format)) {
      return createErrorResponse('formatå‚æ•°åªæ”¯æŒjsæˆ–xml', 400);
    }
    
    if (!/^\d+$/.test(idx) || parseInt(idx) < 0 || parseInt(idx) > 7) {
      return createErrorResponse('idxå‚æ•°å¿…é¡»æ˜¯0-7çš„æ•°å­—', 400);
    }
    
    // è·å–Bingæ•°æ®
    const data = await getBingImageData(idx, mkt, n, format);
    
    if (!data) {
      return createErrorResponse('æ— æ³•è·å–Bingå£çº¸æ•°æ®', 502);
    }
    
    // æ„å»ºå“åº”
    const responseHeaders = {
      ...getCorsHeaders(),
      'Content-Type': format === 'js' ? 'application/json; charset=utf-8' : 'application/xml; charset=utf-8',
      'Cache-Control': 'public, max-age=3600, s-maxage=7200',
      'X-API-Version': '2.0.0',
      'X-Powered-By': 'EdgeOne-API',
    };
    
    const responseBody = format === 'js' ? JSON.stringify(data, null, 2) : data;
    
    console.log(`æˆåŠŸè¿”å›APIæ•°æ®ï¼Œæ ¼å¼: ${format}`);
    
    return new Response(responseBody, {
      status: 200,
      headers: responseHeaders,
    });
    
  } catch (error) {
    console.error('APIè¯·æ±‚å¤„ç†é”™è¯¯:', error);
    return createErrorResponse(`APIæœåŠ¡é”™è¯¯: ${error.message}`, 500);
  }
}

// è·å–Bingå›¾ç‰‡æ•°æ®çš„é€šç”¨å‡½æ•° - å¸¦é‡è¯•æœºåˆ¶
async function getBingImageData(idx, mkt, n, format = 'js') {
  const maxRetries = 3;
  const timeoutMs = 10000; // 10ç§’è¶…æ—¶
  
  // å¤šä¸ªBing APIé•œåƒåœ°å€ï¼Œæé«˜æˆåŠŸç‡
  const bingUrls = [
    `https://www.bing.com/HPImageArchive.aspx?format=${format}&idx=${idx}&n=${n}&mkt=${mkt}`,
    `https://cn.bing.com/HPImageArchive.aspx?format=${format}&idx=${idx}&n=${n}&mkt=${mkt}`,
    `https://global.bing.com/HPImageArchive.aspx?format=${format}&idx=${idx}&n=${n}&mkt=${mkt}`
  ];
  
  for (let attempt = 0; attempt < maxRetries; attempt++) {
    for (let urlIndex = 0; urlIndex < bingUrls.length; urlIndex++) {
      try {
        const bingApiUrl = bingUrls[urlIndex];
        console.log(`å°è¯•è¯·æ±‚ (${attempt + 1}/${maxRetries}): ${bingApiUrl}`);
        
        // ä½¿ç”¨AbortControllerè®¾ç½®è¶…æ—¶
        const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), timeoutMs);
        
        const response = await fetch(bingApiUrl, {
          method: 'GET',
          signal: controller.signal,
          headers: {
            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36',
            'Accept': 'application/json, text/plain, */*',
            'Accept-Language': 'zh-CN,zh;q=0.9,en;q=0.8',
            'Connection': 'keep-alive',
            'Cache-Control': 'no-cache',
            'Pragma': 'no-cache'
          },
          cf: {
            cacheTtl: 3600,
            cacheEverything: true,
            timeout: timeoutMs / 1000, // EdgeOneè¶…æ—¶è®¾ç½®
          }
        });
        
        clearTimeout(timeoutId);
        
        if (!response.ok) {
          console.warn(`è¯·æ±‚å¤±è´¥ ${response.status}: ${bingApiUrl}`);
          continue; // å°è¯•ä¸‹ä¸€ä¸ªURL
        }
        
        if (format !== 'js') {
          return await response.text();
        }
        
        const responseText = await response.text();
        console.log(`æˆåŠŸè·å–æ•°æ®ï¼Œé•¿åº¦: ${responseText.length}`);
        
        let data;
        try {
          data = JSON.parse(responseText);
        } catch (parseError) {
          console.error('JSONè§£æå¤±è´¥:', parseError);
          continue; // å°è¯•ä¸‹ä¸€ä¸ªURL
        }
        
        // éªŒè¯æ•°æ®å®Œæ•´æ€§
        if (!data || !data.images || !Array.isArray(data.images) || data.images.length === 0) {
          console.warn('æ•°æ®æ ¼å¼ä¸å®Œæ•´ï¼Œå°è¯•ä¸‹ä¸€ä¸ªURL');
          continue;
        }
        
        return data;
        
      } catch (error) {
        console.error(`è¯·æ±‚é”™è¯¯ (${attempt + 1}/${maxRetries}, URL ${urlIndex + 1}): ${error.message}`);
        
        // å¦‚æœæ˜¯æœ€åä¸€æ¬¡å°è¯•ä¸”æ˜¯æœ€åä¸€ä¸ªURLï¼ŒæŠ›å‡ºé”™è¯¯
        if (attempt === maxRetries - 1 && urlIndex === bingUrls.length - 1) {
          throw new Error(`æ‰€æœ‰è¯·æ±‚éƒ½å¤±è´¥äº†: ${error.message}`);
        }
        
        // çŸ­æš‚å»¶è¿Ÿåé‡è¯•
        if (urlIndex === bingUrls.length - 1) {
          await new Promise(resolve => setTimeout(resolve, 1000 * (attempt + 1)));
        }
      }
    }
  }
  
  throw new Error('æ— æ³•ä»ä»»ä½•Bing APIè·å–æ•°æ®');
  
  // å¤„ç†å’Œå¢å¼ºå›¾ç‰‡æ•°æ®
  if (data.images && Array.isArray(data.images)) {
    data.images = data.images.map((image, index) => {
      const baseUrl = image.url.startsWith('http') ? image.url : `https://www.bing.com${image.url}`;
      const urlBase = `https://www.bing.com${image.urlbase}`;
      
      return {
        ...image,
        url: baseUrl,
        urlbase: urlBase,
        // ä¸åŒåˆ†è¾¨ç‡çš„å›¾ç‰‡URL
        resolutions: {
          thumbnail: `${urlBase}_400x240.jpg`,
          mobile: `${urlBase}_768x1366.jpg`,
          desktop: `${urlBase}_1920x1080.jpg`,
          uhd: `${urlBase}_UHD.jpg`,
        },
        // ç›´æ¥è®¿é—®å›¾ç‰‡çš„ä»£ç†URL
        proxyUrls: {
          thumbnail: `${new URL('/', globalThis.location || 'https://example.com').href}?idx=${parseInt(idx) + index}&resolution=thumbnail&mkt=${mkt}`,
          mobile: `${new URL('/', globalThis.location || 'https://example.com').href}?idx=${parseInt(idx) + index}&resolution=mobile&mkt=${mkt}`,
          desktop: `${new URL('/', globalThis.location || 'https://example.com').href}?idx=${parseInt(idx) + index}&resolution=1920x1080&mkt=${mkt}`,
          uhd: `${new URL('/', globalThis.location || 'https://example.com').href}?idx=${parseInt(idx) + index}&resolution=uhd&mkt=${mkt}`,
        },
        downloadUrl: `${urlBase}_1920x1080.jpg`,
        previewUrl: `${urlBase}_400x240.jpg`,
        fetchTime: new Date().toISOString(),
        index: parseInt(idx) + index
      };
    });
    
    // æ·»åŠ APIå…ƒä¿¡æ¯
    data.meta = {
      total: data.images.length,
      market: mkt,
      format: format,
      fetchTime: new Date().toISOString(),
      apiVersion: '2.0.0',
      usage: {
        api: 'GET /api/bing-wallpaper - è·å–JSONæ•°æ®',
        image: 'GET / - ç›´æ¥è·å–å›¾ç‰‡',
        parameters: 'idx, mkt, resolution, n, format'
      }
    };
  }
  
  return data;
}

// å¸¦é‡è¯•æœºåˆ¶çš„å›¾ç‰‡è·å–å‡½æ•°
async function fetchImageWithRetry(imageUrl, maxRetries = 3) {
  const timeoutMs = 15000; // 15ç§’è¶…æ—¶
  
  // å›¾ç‰‡å¯èƒ½çš„é•œåƒURL
  const imageUrls = [
    imageUrl,
    imageUrl.replace('www.bing.com', 'cn.bing.com'),
    imageUrl.replace('www.bing.com', 'global.bing.com')
  ];
  
  for (let attempt = 0; attempt < maxRetries; attempt++) {
    for (let urlIndex = 0; urlIndex < imageUrls.length; urlIndex++) {
      try {
        const currentUrl = imageUrls[urlIndex];
        console.log(`è·å–å›¾ç‰‡ (${attempt + 1}/${maxRetries}): ${currentUrl}`);
        
        // è®¾ç½®è¶…æ—¶æ§åˆ¶
        const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), timeoutMs);
        
        const response = await fetch(currentUrl, {
          method: 'GET',
          signal: controller.signal,
          headers: {
            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36',
            'Referer': 'https://www.bing.com/',
            'Accept': 'image/webp,image/apng,image/jpeg,image/png,image/*,*/*;q=0.8',
            'Accept-Language': 'zh-CN,zh;q=0.9,en;q=0.8',
            'Connection': 'keep-alive',
          },
          cf: {
            cacheTtl: 86400, // ç¼“å­˜24å°æ—¶
            cacheEverything: true,
            timeout: timeoutMs / 1000, // EdgeOneè¶…æ—¶è®¾ç½®
            polish: 'lossless', // EdgeOneå›¾ç‰‡ä¼˜åŒ–
            mirage: true, // EdgeOneè‡ªé€‚åº”å›¾ç‰‡
          }
        });
        
        clearTimeout(timeoutId);
        
        if (response.ok) {
          console.log(`å›¾ç‰‡è·å–æˆåŠŸ: ${response.status}, å¤§å°: ${response.headers.get('content-length') || 'unknown'}`);
          return response;
        } else {
          console.warn(`å›¾ç‰‡è¯·æ±‚å¤±è´¥ ${response.status}: ${currentUrl}`);
          continue;
        }
        
      } catch (error) {
        console.error(`å›¾ç‰‡è·å–é”™è¯¯ (${attempt + 1}/${maxRetries}, URL ${urlIndex + 1}): ${error.message}`);
        
        // å¦‚æœæ˜¯æœ€åä¸€æ¬¡å°è¯•ï¼ŒæŠ›å‡ºé”™è¯¯
        if (attempt === maxRetries - 1 && urlIndex === imageUrls.length - 1) {
          throw error;
        }
        
        // çŸ­æš‚å»¶è¿Ÿåé‡è¯•
        if (urlIndex === imageUrls.length - 1) {
          await new Promise(resolve => setTimeout(resolve, 1000 * (attempt + 1)));
        }
      }
    }
  }
  
  throw new Error('æ‰€æœ‰å›¾ç‰‡URLéƒ½æ— æ³•è®¿é—®');
}

// è·å–CORSå¤´éƒ¨
function getCorsHeaders() {
  return {
    'Access-Control-Allow-Origin': '*',
    'Access-Control-Allow-Methods': 'GET, OPTIONS',
    'Access-Control-Allow-Headers': 'Content-Type, Authorization, X-Requested-With',
    'Access-Control-Max-Age': '86400',
    'Access-Control-Expose-Headers': 'X-Image-Title, X-Image-Copyright, X-Image-Date, X-Resolution',
  };
}

// åˆ›å»ºé”™è¯¯å“åº”
function createErrorResponse(message, status = 500) {
  const errorData = {
    error: true,
    message: message,
    status: status,
    timestamp: new Date().toISOString(),
    service: 'EdgeOne Bing Wallpaper Service'
  };
  
  return new Response(JSON.stringify(errorData, null, 2), {
    status: status,
    headers: {
      ...getCorsHeaders(),
      'Content-Type': 'application/json; charset=utf-8',
    },
  });
}# ğŸ¨ éšæœºå£çº¸è®¾ç½®å™¨

ä¸€ä¸ªåŠŸèƒ½å¼ºå¤§çš„éšæœºå£çº¸è®¾ç½®å™¨ï¼Œæ”¯æŒå¤šç§å›¾ç‰‡APIï¼Œå…·æœ‰ç°ä»£åŒ–çš„å›¾å½¢ç”¨æˆ·ç•Œé¢ã€‚

## ğŸ“¸ ç•Œé¢é¢„è§ˆ
